{
  "name": "path-to-repo/stomp",
  "description": "Object wrapper for stomp-php lib",
  "keywords": ["stomp", "message"],
  "type": "symfony-bundle",
  "license": "MIT",
  "version": "1.0",

  "repositories": [
    {
      "type": "vcs",
      "url": "set path to repo git here"
    }
  ],

  "require": {
    "php": "^7.1.3",
    "psr/log": "^1",
    "stomp-php/stomp-php": "@stable"
  },
  "autoload": {
    "psr-4": {
      "Aimchat\\StompBundle\\": ""
    }
  },
  "scripts": {
    "auto-scripts": {
      "cache:clear": "symfony-cmd",
      "assets:install %PUBLIC_DIR%": "symfony-cmd"
    }
  }
}
